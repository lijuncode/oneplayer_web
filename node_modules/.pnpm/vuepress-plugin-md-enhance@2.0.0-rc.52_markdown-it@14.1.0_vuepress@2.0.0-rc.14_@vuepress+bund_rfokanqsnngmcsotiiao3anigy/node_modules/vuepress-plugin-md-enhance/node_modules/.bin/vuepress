#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lijun/Work/vuepree-hope/vuepress-oneplayer/node_modules/.pnpm/vuepress@2.0.0-rc.14_@vuepress+bundler-vite@2.0.0-rc.14_@types+node@22.5.1_sass@1.77.8__vue@3.4.38/node_modules/vuepress/bin/node_modules:/Users/lijun/Work/vuepree-hope/vuepress-oneplayer/node_modules/.pnpm/vuepress@2.0.0-rc.14_@vuepress+bundler-vite@2.0.0-rc.14_@types+node@22.5.1_sass@1.77.8__vue@3.4.38/node_modules/vuepress/node_modules:/Users/lijun/Work/vuepree-hope/vuepress-oneplayer/node_modules/.pnpm/vuepress@2.0.0-rc.14_@vuepress+bundler-vite@2.0.0-rc.14_@types+node@22.5.1_sass@1.77.8__vue@3.4.38/node_modules:/Users/lijun/Work/vuepree-hope/vuepress-oneplayer/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/lijun/Work/vuepree-hope/vuepress-oneplayer/node_modules/.pnpm/vuepress@2.0.0-rc.14_@vuepress+bundler-vite@2.0.0-rc.14_@types+node@22.5.1_sass@1.77.8__vue@3.4.38/node_modules/vuepress/bin/node_modules:/Users/lijun/Work/vuepree-hope/vuepress-oneplayer/node_modules/.pnpm/vuepress@2.0.0-rc.14_@vuepress+bundler-vite@2.0.0-rc.14_@types+node@22.5.1_sass@1.77.8__vue@3.4.38/node_modules/vuepress/node_modules:/Users/lijun/Work/vuepree-hope/vuepress-oneplayer/node_modules/.pnpm/vuepress@2.0.0-rc.14_@vuepress+bundler-vite@2.0.0-rc.14_@types+node@22.5.1_sass@1.77.8__vue@3.4.38/node_modules:/Users/lijun/Work/vuepree-hope/vuepress-oneplayer/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../vuepress@2.0.0-rc.14_@vuepress+bundler-vite@2.0.0-rc.14_@types+node@22.5.1_sass@1.77.8__vue@3.4.38/node_modules/vuepress/bin/vuepress.js" "$@"
else
  exec node  "$basedir/../../../../../vuepress@2.0.0-rc.14_@vuepress+bundler-vite@2.0.0-rc.14_@types+node@22.5.1_sass@1.77.8__vue@3.4.38/node_modules/vuepress/bin/vuepress.js" "$@"
fi
